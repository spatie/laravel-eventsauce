{
    "name": "spatie/laravel-eventsauce",
    "description": "Use EventSauce in Laravel apps",
    "keywords": [
         "spatie",
        "laravel-eventsauce",
        "event sourcing",
        "eventsauce",
        "events"
    ],
    "homepage": "https://github.com/spatie/laravel-eventsauce",
    "license": "MIT",
    "authors": [    
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Dries Vints",
            "email": "dries.vints@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "eventsauce/eventsauce": "^0.5.0",
        "illuminate/bus": "^5.7",
        "illuminate/container": "^5.7",
        "illuminate/queue": "^5.7",
        "illuminate/support": "^5.7",
        "ramsey/uuid": "^3.8",
        "spatie/temporary-directory": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7.6",
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelEventSauce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelEventSauce\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LaravelEventSauce\\EventSauceServiceProvider"
            ]
        }
    }
}
